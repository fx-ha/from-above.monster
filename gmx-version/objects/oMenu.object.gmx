    if ButtonPressed("Credits") {
        Mode = "Credits"
        room_goto(rCredits)
    }
    
    if ButtonPressed("Quit") {
        game_end()
    }
} else if Mode = "Game" {

    if instance_exists(oPlayer) {

        BarIndent = 50  // 50 Pixel vom linken Rand entfernt
        BarWidth = view_wport - (BarIndent * 2) // ganze Bildschirmbreite minus 100 Pixel 
        BarHeight = 20 
        BarTop = (view_hport - BarIndent) - BarHeight
        
        LuftFraction = oPlayer.Luft / oPlayer.LuftVoll
        InsanityFraction = oPlayer.Insanity / oPlayer.FullInsanity
        
        draw_set_colour(c_green)
        draw_set_alpha(0.7)
        draw_rectangle(BarIndent, BarTop, BarIndent + BarWidth, BarTop + BarHeight, true)
        draw_rectangle(BarIndent, BarTop, BarIndent + (BarWidth * LuftFraction), BarTop + BarHeight, false)
        
        draw_set_colour(c_red)
        draw_rectangle(BarIndent, BarTop + BarHeight, BarIndent + BarWidth, BarTop + BarHeight*2, true)
        draw_rectangle(BarIndent, BarTop + BarHeight, BarIndent + (BarWidth * InsanityFraction), BarTop + BarHeight*2, false)
